<p id="notice"><%= notice %></p>

<h1>Annual Events</h1>

<table>
  <thead>
    <tr>
      <th>Filename</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @annual_events.each do |annual_event| %>
      <tr>
        <td><%= annual_event.filename %></td>
        <td><%= link_to '閲覧', annual_event %></td>
        <td><%= link_to '編集', edit_annual_event_path(annual_event) %></td>
        <td><%= link_to '削除', annual_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to '登録', new_annual_event_path %>
