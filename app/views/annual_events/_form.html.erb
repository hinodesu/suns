<%= form_with(model: annual_event, local: true) do |form| %>
  <% if annual_event.errors.any? %>

    <div id="error_explanation">
      <h2>
          <%= pluralize(annual_event.errors.count, "error") %> 
          prohibited this annual_event from being saved:
        </h2>

      <ul>
        <% annual_event.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class:fs20>
    <div class="field">
    <%= form.label :ファイルを選択 %>
  </div>
    <%= form.file_field :filename %>
  </div>

  <span>
    <div class="actions">
      <%= submit_tag "登録", class:"submit" %>
      <%= link_to '戻る', home_top_path, class:"submit" %>
    </div>
  </span>
<% end %>
